//Take n numbers from the user, store them in an array, and print them in reverse order.


import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("The n are : ");
        int n = sc.nextInt();

        int[] numbers = new int[n];//Creat array of size n

        System.out.println("The numbers are: ");
        for (int i = 0; i < n; i++) {
            numbers[i] = sc.nextInt();
        }

        System.out.println("The numbers in revers are: ");
        for (int i = n - 1; i >= 0; i--) {
            System.out.print(numbers[i] + " ");
        }

    }
}
